{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.js"
        },
        {
            "src": "/reset-password.html",
            "dest": "/public/reset-password.html"
        }
    ]
}